include "std.porth"

macro limit mem end

argc 2 < if
  "Usage: seq <limit>\n" eputs
  "ERROR: no limit is provided\n" eputs
  1 exit
end

1 nth_argv
while dup , 0 != do
  dup , '0' < over , '9' > or if
    "ERROR: `" eputs
    1 nth_argv strlen 1 nth_argv eputs
    "` is not a valid integer\n" eputs
    1 exit
  end

  limit ,64 10 *
  over , '0' -
  +
  limit swap .64

  1 +
end drop

0 while dup limit ,64 < do
  dup print
  1 +
end drop

