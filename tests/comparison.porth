include "std.porth"

// Equals
69 420  = print
69 69   = print

"------------------------------\n" stdout write drop

// Greater than
420 69  > print
69 420  > print

"------------------------------\n" stdout write drop

// Less than
420 69  < print
69 420  < print

"------------------------------\n" stdout write drop

// Greater or Equals
420 69 >= print
69 420 >= print
69 69  >= print

"------------------------------\n" stdout write drop

// Less or Equals
420 69 <= print
69 420 <= print
69 69  <= print

"------------------------------\n" stdout write drop

// Not Equals
69 69  != print
420 69 != print

