include "std.porth"

memory a sizeof(Str) end
"1234" a !Str

memory b sizeof(Str) end
"abcd" b !Str

a @Str try-parse-int if print else
    drop
    "`" eputs a @Str eputs "` is not a number\n" eputs
end

b @Str try-parse-int if print else
    drop
    "`" eputs b @Str eputs "` is not a number\n" eputs
end
